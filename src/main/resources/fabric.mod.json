{
  "schemaVersion": 1,
  "id": "albow",
  "version": "${version}",
  "name": "A Little Bit of Weapons",
  "description": "A Minecraft Mod which adds weapons for the ALBOE modpack.",
  "authors": ["Timefall Development", "Chronosacaria"],
  "contact": {
    "homepage": "",
    "sources": "https://github.com/TimefallDevelopment/AlboeMythicArmory",
    "issues": "https://github.com/TimefallDevelopment/AlboeMythicArmory/issues"
  },
  "license": "MIT",
  "icon": "assets/albow/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "dev.timefall.albow.client.AlbowDataGenerator"
    ],
    "client": [
      "dev.timefall.albow.client.AlbowClient"
    ],
    "main": [
      "dev.timefall.albow.Albow"
    ]
  },
  "mixins": [
    "albow.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "fzzy_config": ">=${fzzy_config_version}"
  }
}
